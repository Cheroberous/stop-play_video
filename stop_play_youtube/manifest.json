{
  "manifest_version": 3,
  "name": "Manage video",
  "version": "1.2",
  "description": "Shows a window to manage video",

  "background": {
    "service_worker": "background.js"
  }, 

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "run_at" : "document_start",
      "js": ["contentScript.js"]
    }
  ],
  
  "host_permissions": ["https://www.youtube.com/*"],

  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  }

  
}
